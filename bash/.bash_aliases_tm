
alias trh='echo -e "transmission-daemon aliases: \nstart - trs\nstop - trb\nrestart -  trr\nstatus -  trl\nconfig -  trc\ndownloaded -  trd\ntorrents - trt\netc - tre"'
alias trs='sudo service transmission-daemon start'
alias trb='sudo service transmission-daemon stop'
alias trr='sudo service transmission-daemon restart'
alias tra='sudo service transmission-daemon status'
alias trc='sudo vim /etc/transmission-daemon/settings.json'
alias trch='sudo vim /home/arcadas/.config/transmission-daemon/settings.json'
alias trdp='sudo chmod -R 777 /media/nas/torrent'
alias trdl='ls /media/arcadas/nas/torrent'
alias trdc='cd /media/arcadas/nas/torrent'
alias tre='sudo vim /etc/default/transmission-daemon'
alias trl='tail -f /var/log/syslog'
alias trls='tail -f /var/log/transmission.log'
